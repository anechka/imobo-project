		script(type='text/javascript').
			google.maps.event.addDomListener(window, 'load', function () {
				var places = new google.maps.places.Autocomplete(document.getElementById('busca_principal'), { types: ['geocode'], componentRestrictions: {country: "br"} });
				google.maps.event.addListener(places, 'place_changed', function () {
					var place = places.getPlace();
					var address = place.formatted_address;

					var vals = address.split(",");
					var cidade = vals[vals.length - 2];
					var bairro = vals[vals.length - 3];


					var latitude = place.geometry.location.k;
					var longitude = place.geometry.location.D;
					var mesg = "Address: " + address;
					mesg += "\nLatitude: " + latitude;
					mesg += "\nLongitude: " + longitude;
					mesg += "\nbairro: " + bairro;
					mesg += "\ntudo: " + address;
					alert(mesg);
			});
			});


